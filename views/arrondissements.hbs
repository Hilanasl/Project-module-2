<h1>Explore the {{cards.0.arrond}}</h1>

<section class='container'>
        {{#each cards}}
        <div class='card'>
        <img src="{{this.image}}"/>
        <p>{{this.title}}</p>
        <p>Arrondissement: {{this.arrond}}</p>
        <p>{{this.category}}</p>
        <a href='/arrond/{{this.arrond}}/{{this._id}}'>See more info</a>
        </div>
        {{/each}}
</section>