<h1>{{cards.0.arrond}} </h1>    

<div class='myfave' onclick="fav()">
<i id="favIcon"  class="far fa-star">  </i>
</div>

<section class='container'>
        {{#each cards}}
        <div class='card'>
        <img src="{{this.image}}"/>
        <p>{{this.title}}</p>
        <p>{{this.location}}</p>
        <a href='/arrond/{{this.arrond}}/{{this._id}}'>See more info</a>

        </div>
        {{/each}}


</section>