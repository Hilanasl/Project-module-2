<section class='container'>
        {{#each catCards}}
        <div class='card'>
        <img src="{{this.image}}"/>
        <div class='card-text'>
        <p>{{this.title}}</p>
        {{#if this.author}}
        <p>Added by: {{this.author.username}}</p>
        {{/if}}
        </div>
        <a href='/arrond/{{this.category}}/{{this._id}}' class='info'>See more info</a>
        </div>
        {{/each}}
</section>
