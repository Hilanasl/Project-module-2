<section class='container'>
        {{#each allCards}}
        <div class='card'>
                <img src="{{this.image}}" />
                <p>{{this.title}}</p>
                <p>{{this.category}}</p>
                <a href='/arrond/{{this.category}}/{{this._id}}'>See more info</a>
        </div>
        {{/each}}
</section>